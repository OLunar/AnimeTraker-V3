d-color: rgba(0, 0, 0, 0.8);\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.3);\r\n}\r\n\r\n.nav-wrapper {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    padding: 0 20px;\r\n}\r\n\r\n.brand-logo {\r\n    color: #ff7043;\r\n    font-size: 1.5em;\r\n    font-weight: bold;\r\n    text-transform: uppercase;\r\n}\r\n\r\n.container {\r\n    flex: 1;\r\n    padding: 20px;\r\n    width: 100%;\r\n}\r\n\r\n.input-field input[type=\"text\"] {\r\n    border: 2px solid #ff7043;\r\n    background-color: rgba(255, 255, 255, 0.2);\r\n    color: #fff;\r\n    padding: 10px;\r\n    border-radius: 8px;\r\n    margin-bottom: 20px;\r\n}\r\n\r\n.input-field input[type=\"text\"]:hover, \r\n.input-field input[type=\"text\"]:focus {\r\n    background-color: rgba(255, 255, 255, 0.4);\r\n    outline: none;\r\n    border-color: #ff5722;\r\n}\r\n\r\n.input-field label {\r\n    color: #ff7043;\r\n}\r\n\r\n#add-anime-btn, .remove-btn {\r\n    background-color: #ff7043;\r\n    border-radius: 50%;\r\n    transition: transform 0.3s ease-in-out;\r\n}\r\n\r\n#add-anime-btn:hover, .remove-btn:hover {\r\n    transform: scale(1.2);\r\n}\r\n\r\n.card {\r\n    border: none;\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.3);\r\n    margin: 15px;\r\n    transition: transform 0.3s ease-in-out;\r\n    border-radius: 10px;\r\n    overflow: hidden;\r\n    background-color: rgba(255, 255, 255, 0.1);\r\n}\r\n\r\n.card-container {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: center; \r\n}\r\n\r\n.card:hover {\r\n    transform: scale(1.05);\r\n}\r\n\r\n.card-image img {\r\n    width: 100%;\r\n    height: auto;\r\n}\r\n\r\n.card-title {\r\n    font-size: 1.2em;\r\n    font-weight: bold;\r\n    color: #ff7043;\r\n}\r\n\r\n.card-content {\r\n    padding: 15px;\r\n    color: #fff;\r\n}\r\n\r\n.remove-btn {\r\n    position: absolute;\r\n    top: 10px;\r\n    right: 10px;\r\n    background-color: #ff7043;\r\n    color: #fff;\r\n    cursor: pointer;\r\n}\r\n\r\nbody::before {\r\n    content: '';\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    right: 0;\r\n    bottom: 0;\r\n    background: rgba(0, 0, 0, 0.5);\r\n    pointer-events: none;\r\n}\r\n\r\n.modal-content {\r\n    background-color: #282828;\r\n    border-radius: 8px;\r\n    padding: 20px;\r\n    color: #fff;\r\n}\r\n\r\n.modal-footer .btn-flat {\r\n    background-color: #ff7043;\r\n    color: #fff;\r\n}\r\n\r\n.modal-footer .btn-flat:hover {\r\n    background-color: #ff5722;\r\n}\r\n\r\n@media (max-width: 768px) {\r\n    .banner-text {\r\n        font-size: 1.2em;\r\n    }\r\n\r\n    .card {\r\n        width: 100%;\r\n    }\r\n\r\n    #add-anime-btn {\r\n        right: 10px;\r\n        bottom: 10px;\r\n    }\r\n    \r\n}\r\n\r\n.tabs .tab a {\r\n    color: #ff7043; \r\n}\r\n\r\n.tabs .tab a:hover {\r\n    background-color: #ff7043;\r\n    color: #fff;\r\n}\r\n\r\n.tabs .tab a.active {\r\n    background-color: #ff7043;\r\n    color: #fff;\r\n}\r\n\r\n.tabs .indicator {\r\n    background-color: #ff7043; \r\n}\r\n"},"lineCount":null}},"error":null,"hash":"33f2d8bb55b23e50f0317a8d081928fc","cacheData":{"env":{}}}